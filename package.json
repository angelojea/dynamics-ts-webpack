{
  "name": "dynamics-ts-webpack",
  "version": "1.0.0",
  "author": "Angel Ojea",
  "description": "A sample of how to implement frontend (javascript) development in Dynamics 365 using typescript",
  "main": "index.js",
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "build": "npx webpack --config webpack.config.dev.js",
    "build:prod": "npx webpack --config webpack.config.js",
    "commit": "set /P comment=\"Enter your commmit comment: \" && npm run finish-commit",
    "finish-commit": "git add . && git commit -m \"%comment%\" && git push",
    "lint": "npx eslint . --ext .ts",
    "new-module": "node scaffold\\module.js",
    "new-lib": "node scaffold\\library.js",
    "generate-models": "node scaffold\\models.js"
  },
